<?xml version="1.0" encoding="UTF-8"?>
<description xmlns:d="http://openoffice.org/extensions/description/2006"
             xmlns:xlink="http://www.w3.org/1999/xlink"
             xmlns="http://openoffice.org/extensions/description/2006">
    <version value="2021.11.01"/>
    <identifier value="org.openoffice.en.hunspell.dictionaries"/>
    <display-name>
        <name lang="en-US">English spelling dictionaries, hyphenation rules, thesaurus, and grammar
